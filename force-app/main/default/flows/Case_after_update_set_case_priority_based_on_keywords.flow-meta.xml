    <decisions>
        <name>Check_case_priority</name>
        <label>Check case priority</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <defaultConnector>
            <targetReference>Keyword_loop</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Case_priority_Keyword_priority</name>
            <conditionLogic>1 AND (2 OR 3)</conditionLogic>
            <conditions>
                <leftValueReference>casePriority</leftValueReference>
                <operator>LessThan</operator>
                <rightValue>
                    <elementReference>loopKeywordPriority</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Subject</leftValueReference>
                <operator>Contains</operator>
                <rightValue>
                    <elementReference>Keyword_loop.Key_word__c</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Description</leftValueReference>
                <operator>Contains</operator>
                <rightValue>
                    <elementReference>Keyword_loop.Key_word__c</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Assign_new_peiority</targetReference>
            </connector>
            <label>Case priority &lt; Keyword priority</label>
        </rules>
    </decisions>
    <decisions>
        <name>Set_priority_based_on_category_and_subcategory</name>
        <label>Set priority based on category and subcategory</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <defaultConnector>
            <targetReference>Get_all_keywords</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Case Priority Very Low</defaultConnectorLabel>
        <rules>
            <name>Case_Priority_Very_HIgh</name>
            <conditionLogic>1 OR 2 OR 3 OR (4 AND (5 OR 6))</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Category__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Deposits</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Category__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
